function replaceBooks (innerHTML) {
  const books = document.getElementById('books-tbody');
  books.innerHTML = innerHTML;
}

function replaceKaminari (innerHTML) {
  const kaminari = document.getElementById('kaminari-infos');
  kaminari.innerHTML = innerHTML;
}

function replaceNavBar (innerHTML) {
  const navbar = document.getElementById('nav-bar-books');
  navbar.innerHTML = innerHTML;
}

replaceBooks("<%= j render @books %>");
replaceKaminari("<%= j render 'layouts/shared/kaminari_infos', list: @books, remote: true %>")
replaceNavBar("<%= j render 'books/nav_bar' %>")

